@{
	ViewBag.Title = "Subscription";
}

<div class="body-container" ng-controller="SubscriptionController" id="Subscription" layout="column" ng-cloak="">
	
	<md-toolbar class="toolbar" layout-fill flex>
		<div class="md-toolbar-tools">
			<span id="backTT" flex></span>
			<md-button class="md-icon-button" aria-label="Notifications">
				<i class="material-icons">&#xE7F4;</i>
			</md-button>
			<span class="mdl-badge mdl-badge--overlap" data-badge="2"></span>
			<md-button class="md-icon-button" aria-label="Messages">
				<i class="material-icons">&#xE0BF;</i>
			</md-button>
			<md-button class="md-icon-button" aria-label="Settings">
				<i class="material-icons">&#xE8B8;</i>
			</md-button>
			<md-button class="md-icon-button" aria-label="Help">
				<i class="material-icons">help</i>
			</md-button>
			<md-button class="md-icon-button" aria-label="Logout">
				<i class="material-icons">&#xE8AC;</i>
				<md-tooltip md-direction="down">Logout</md-tooltip>
			</md-button>
		</div>
	</md-toolbar>

	<md-content class="main-cnt" layout-padding flex>
		<div class="table-div">
			<table id="subscriptionTable" class="ui celled table" cellspacing="0" width="100%"></table>
		</div>
		
	</md-content>
	
</div>

@section scripts {
	<script type="text/javascript" src="~/js/SubscriptionController.js"></script>
}
